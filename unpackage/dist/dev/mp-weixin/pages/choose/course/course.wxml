<view class="content"><header vue-id="583abff6-1" headTitle="{{headTitle}}" bind:__l="__l"></header><scroll-view class="container" style="{{'height:'+(scrollH+'rpx')+';'}}" scroll-y="true"><u-button vue-id="583abff6-2" custom-style="{{customStyle}}" data-event-opts="{{[['^click',[['submit']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">保存选科结果</u-button><block wx:for="{{grpList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="subject-group"><view class="group-title"><view class="title">{{item.name}}</view><u-button vue-id="{{'583abff6-3-'+i}}" custom-style="{{customStyle}}" disabled="{{item.checklock}}" plain="{{true}}" data-event-opts="{{[['^click',[['courses',['$0'],[[['grpList','',i,'id']]]]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">进入选科</u-button></view><view class="group-main"><view style="margin-bottom:10rpx;"><text class="grounp-text">至少选择：<text class="grounp-text" style="color:#189e95;">{{item.min+"科"}}</text></text><text class="grounp-text">至多选择：<text class="grounp-text" style="color:#189e95;">{{item.max+"科"}}</text></text></view><view><text class="grounp-text">分组描述：<text class="grounp-text" style="color:#189e95;">{{item.remark}}</text></text></view></view><block wx:if="{{item.check.length!==0}}"><view class="group-result"><view class="grounp-text">分组选科结果展示信息：</view><view class="result-list"><block wx:for="{{item.check}}" wx:for-item="value" wx:for-index="idx" wx:key="idx"><view class="result-item">{{''+value.value+''}}</view></block></view></view></block></view></block></scroll-view><toast class="vue-ref" vue-id="583abff6-4" data-ref="toast" bind:__l="__l"></toast><toptips class="vue-ref" vue-id="583abff6-5" data-ref="toptips" bind:__l="__l"></toptips></view>