.content .container {
  box-sizing: border-box;
  padding: 20rpx 28rpx 20rpx;
}
.content .container .subject-group {
  padding: 40rpx;
  box-shadow: 0 0 8rpx #f0f0f0;
  border: 1px solid #f1f1f1;
  margin-bottom: 40rpx;
  margin-top: 20rpx;
}
.content .container .subject-group .group-title {
  border-bottom: 1px solid #d7d7d7;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
  padding-bottom: 10rpx;
}
.content .container .subject-group .group-title .title {
  font-size: 48rpx;
  color: #333;
}
.content .container .group-main {
  padding: 40rpx 0;
}
.content .container .group-main .grounp-text {
  color: #4a4a4a;
  font-size: 28rpx;
}
.content .container .group-result {
  padding-top: 26rpx;
  border-top: 1px dashed #d7d7d7;
}
.content .container .group-result .result-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.content .container .group-result .result-list .result-item {
  width: 30%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin-top: 26rpx;
  color: #f4a509;
  font-size: 28rpx;
  border-radius: 4rpx;
  border: 1px solid #f4a509;
  margin-right: 20rpx;
}

